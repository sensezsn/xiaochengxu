.tittle-wrap{
  padding: 12px 10px;
  font-size: 12px;
  color: #333;
  border-bottom: 10px solid #efefef;
}
.grey{
  color: #b3b3b3;
}
.label-list .label-item{
  padding: 0 10px;
  height: 40px;
  line-height: 40px;
  font-size: 12px;
  border-bottom: 1px solid#efefef;
  position: relative;
}
.label-list .label-item .icon{
  width: 16px;
  height: 16px;
  display: inline-block;
  vertical-align: middle;
  margin-right: 10px;
  position: relative;
  top: -2px;
}
.label-list .label-item .input{
  display: inline-block;
  position: relative;
  top: 7px;
}
.add-label {
  font-size: 12px;
  padding: 20px;
  color: #10cbd2;
}
.tips-wrap .red{
  background: #ff342e;
  color: #fff;
  height: 40px;
  line-height: 40px;
  display: inline-block;
  width: 140rpx;
  text-align: center;
  letter-spacing: 4px;
  position: absolute;
  right: 0;
  /* position: ; */
}
.add-label .icon{
  width: 10px;
  height: 10px;
  margin-right: 6px;
}