.renwu-container{
  background: #efefef;
  position: fixed;
  top: 0;
  bottom: 0;
  width: 100%;
  overflow: scroll;
}
.form-wrap{
  background: #fff;
}
.form-wrap .form-item{
  height: 40px;
  line-height: 40px;
  font-size: 12px;
  position: relative;
  background: #fff;
  border-bottom: #efefef;
}
.form-wrap .form-item .label{
  color: #333;
  width: 200rpx;
  position: absolute;
  left: 10px;
  height: 40px;
  line-height: 40px;
  display: inline-block;  
}

.form-wrap .form-item .input-wrap{
  display: inline-block;
  width: 100%;
  text-align: right;
  height: 40px;
  line-height: 40px;
  padding-left: 200rpx;
  padding-right: 20px;
  box-sizing: border-box;
}
input::placeholder{
  color: #b3b3b3;
}
.form-item .icon-wrap{
  display: inline-block;
  width: 40px;
  height: 40px;
  line-height: 40px;
  margin-left: -20px;
}
.form-item .icon-wrap image{
  display: inline-block;
}
.form-item .icon-wrap .addImage{
  width: 30px;
  height: 30px;

}
.form-item .icon-wrap .icon{
  height: 8px;
  width: 8px;
  margin-left: 20px;
  position: relative;
  top: -4px;
}
.tips-wrap{
  padding: 10px;
  background-color: #efefef;
}
.tips-wrap  .tips-tittle{
  font-size: 12px;
}
.tips-wrap .li-item{
  margin-top: 4px;
  font-size: 12px;
  color: #333;
}
.btn-wrap-top{
  padding: 140rpx 0 70rpx 0;

}