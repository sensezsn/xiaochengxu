.paddingBottom{
  padding-bottom: 200rpx;
}
.img-wrap{
  position: absolute;
  top: 0;
  bottom: 0;
  width: 100%;
  background:#f3f3f3;
}
.detail-img{
  position: absolute;
  width: 100%;
  height: 800rpx;
  top: 50%;
  margin-top: -400rpx;
}
.btn-position{
  position: fixed;
  bottom: 0;
  width: 100%;
  padding: 0;
}
.btn-wrap.btn-position .btn{
  width: 100%;
  border-radius: 0;
}