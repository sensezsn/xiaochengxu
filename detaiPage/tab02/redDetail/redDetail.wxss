/* detaiPage/tab02/drainage/drainage.wxss */.renwu-container{
  background: #efefef;
  position: fixed;
  top: 0;
  bottom: 0;
  width: 100%;
  overflow: scroll;
}

.top-wrap{
  height: 40px;
  line-height: 40px;
  color: #333;
  background-color: #fff;
  padding: 0 10px;
  overflow: hidden;
}
.img-wrap{
  padding: 10px;
  background: #fff;
}
.image-con{
  padding: 10px;
  padding-bottom: 0px;
}
.image-con image{
  width: 100%;
  height: 240rpx;
  border-radius: 20px;

}
 .tittle{
  font-size: 12px;
}
.top-wrap .small-titlle{
  font-size: 26rpx;
  margin-left: 10px;
}
.tips-detail{
  padding: 10px;
  background: #fff;
  margin-top: 12px;
}
.tips-detail .tips-item{
  height: 36px;
  line-height: 36px;
  overflow: hidden;
}
.tips-detail .tips-item .small-tittle{
  font-size: 28rpx;
  color: #333;
  padding-left: 10px;
}
.tips-detail .tips-item .tittle{
  display: inline-block;
  width: 160rpx;
}
.tittle-wrap{
  text-align: center;
}
.tittle-wrap .tittle{
  font-size: 14px;
}
.tittle-wrap .small-tittle{
  font-size: 28rpx;
  margin-top: 8px;
}

.btn-wrap-top{
  margin-top: 14px;  
}
.btn-wrap-top .link-btn{
  background-color: #ebebeb;
  color: #b7b7b7;
}