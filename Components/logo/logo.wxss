.cti-baoBox {
  position: fixed;
  left: 0;
  right: 0;
  z-index: 98;
  height: 200rpx;
  pointer-events: none;
}

.cti-masterHeadTop {
  position: relative;
  z-index: 98;
  height: 200rpx;
  transition: all 0.3s ease-in;
  transform: translateY(0);
  opacity: 1;
}

.cti-headlogoImg {
  width: 256rpx;
  height: 46rpx;
}

.cti-headlogoImgBlack {
  width: 256rpx;
  height: 46rpx;
}

.restLogo.up .cti-line,
.restLogoBlack .cti-line {
  background: #001e50;
}

.cti-LogoBg.down {
  transform: translateY(-200%);
  opacity: 0;
}

.cti-masterHeadTop.down {
  transform: translateY(-200%);
  opacity: 0;
  pointer-events: none;
}

.cti-masterHeadTopBlack.down {
  transform: translateY(-200%);
  opacity: 0;
}

.cti-masterHeadTopBlack {
  position: fixed;
  top: 98px;
  left: 0;
  right: 0;
  z-index: 98;
  width: 100%;
  height: 200rpx;
  transition: all 0.3s ease-in;
  transform: translateY(0);
  opacity: 1;
}

.cti-masterTop {
  position: relative;
  overflow: hidden;
  width: 100%;
  height: 60px;
  z-index: 5;
  top: -100%;
}

.cti-masterTop.bg {
  background: hsla(0, 0%, 100%, 0.9);
}

.cti-LogoBg {
  background: #fff;
  position: relative;
  z-index: 4;
  height: 200rpx;
  width: 100%;
  top: -160%;
  transition: all 0.3s ease-in;
  transform: translateY(0);
  opacity: 1;
}

.cti-classwhite {
  background: hsla(0, 0%, 100%, 0.9);
  position: fixed;
  top: 64px;
  left: 0;
  right: 0;
  z-index: 90;
  width: 100%;
  height: 120rpx;
}

.cti-masterTopBlack {
  position: fixed;
  overflow: hidden;
  width: 100%;
  height: 60px;
  top: 64px;
  left: 0;
  right: 0;
  z-index: 999;
}

.cti-headlogo {
  position: absolute;
  top: 30%;
  right: 3%;
  width: 256rpx;
  height: 46rpx;
  background-size: 100%;
}

.cti-headlogoBlack {
  position: absolute;
  top: 30%;
  right: 3%;
  width: 256rpx;
  height: 46rpx;
  background-size: 100%;
}

.cti-masterScrollIcon {
  align-items: center;
  position: absolute;
  top: 37px;
  left: 0;
  z-index: 98;
  width: 100%;
  height: 48px;
  opacity: 1;
}

.cti-masterScrollIcon.bg {
  background-color: #fff;
}

.cti-line {
  height: 2px;
  background-color: #fff;
  margin-top: 22px;
  width: 43%;
}

.cti-lineL {
  float: left;
}

.cti-lineR {
  float: right;
}

.lineIcon {
  float: left;
  width: 14%;
  height: 64rpx;
  text-align: center;
  line-height: 0;
  margin-top: 1.5%;
  display: inline-block;
}

.cti-logoTu {
  width: 64rpx;
  height: 64rpx;
}

.cti-lineBlack {
  height: 2px;
  background-color: #001e50;
  margin-top: 22px;
  width: 43%;
}