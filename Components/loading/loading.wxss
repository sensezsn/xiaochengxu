/* 遮罩 */
.hLoading {
  position: fixed;
  left: 0;
  top: 0;
  height: 100%;
  width: 100%;
  background-color: rgba(241, 241, 241, 1);
  z-index: 996;
}

.hLoading__img {
  position: fixed;
  top: 50%;
  left: 50%;
  width: 80rpx;
  height: 80rpx;
  margin-left: -40rpx;
  margin-top: -40rpx;
}