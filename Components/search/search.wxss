.search-wrap{
  background: #efefef;
  padding: 8px 10px;
}
.search-wrap .input-wrap{
  border-radius: 10px;
  overflow: hidden;
  position: relative;
}
.search-wrap .input-wrap input{
  background: #fff;
  padding: 8px 14px;
  display: inline-block;
  border-radius: 6px;
  height: 30px;
  line-height: 30px;
  width: 100%;
  text-align: center;
}
.input-placeholder{
  color:rgb(207,207,207);
  font-size:14px; 
  text-align: center;
}
.pre-see{
  position: absolute;
  right: 10px;
  top: 10px;
  color: #10cbd2;
}
.pre-see .icon{
  width: 14px;
  height: 10px;
  margin-right: 4px;
}
.input-placeholder::before{
  display: inline-block;
  content: '';
  
  width: 14px;
  height: 14px;
  overflow: hidden;
  position: relative;
  right: 6px;
  top: 1px;
  background-repeat:no-repeat; 
  background-size:contain; 
  /* height: 600rpx; */
  background-size: 100% 100%;
  background-image: url('data:image/png;base64,');
}
